{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended"
       
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module",
        "impliedStrict": true
    },
    
    "rules": {
        "indent":["error", 4],
        "linebreak-style": ["error", "unix"],
        "semi": ["error", "always"],
        "quotes": ["error", "double"],
        "arrow-body-style": ["error", "always"]
    }
}
